e.nombre,
e.apellido1,
e.apellido2,
e.email,
e.puesto,
e.ID_oficina,
COUNT (p.ID_pedido) AS total_pedidos_entregados
FROM empleado e
join pedido p ON e.ID_empleado = p.ID_cliente
WHERE p.estado = 'entregado'
Group by
e.ID_empleado,
e.nombre,
e.apellido1,
e.apellido2,
e.email,
e.puesto,
e.ID_oficina
ORDER BY
total_pedidos_entregados DESC;</property>
