              <properties>
                <property
                  dataType="System.String"
                  description="Especifica la instrucción SQL utilizada por el componente para extraer datos."
                  expressionType="Notify"
                  name="SqlCommand"
                  UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor">SELECT 
    ROW_NUMBER() OVER (ORDER BY ped.ID_pedido_o) AS ID_venta,
    ped.ID_pedido_o AS ID_pedido,
    ped.ID_cliente AS ID_cliente,
    e.ID_empleado_o AS ID_empleado,
    dp.ID_producto AS ID_producto,
    dp.cantidad AS cantidad_producto_vendido,
    ISNULL(CONVERT(VARCHAR, ped.fecha_entrega), 'NA') AS fecha_venta,
    (dp.cantidad * dp.precio_unidad) AS total_venta
FROM 
    [Stagin].[dbo].[Destino_Pedidos] ped
INNER JOIN 
    [Stagin].[dbo].[Destino_Detalle_Pedido] dp ON ped.ID_pedido = dp.ID_pedido_o
INNER JOIN 
    [Stagin].[dbo].[Destino_Cliente] c ON ped.ID_cliente = c.ID_cliente_o
INNER JOIN 
    [Stagin].[dbo].[Destino_Empleados] e ON c.ID_empleado_rep_ventas = e.ID_empleado_o
INNER JOIN 
    [Stagin].[dbo].[Destino_Productos] prod ON dp.ID_producto = prod.ID_producto_o
ORDER BY 
    ped.ID_pedido;</property>
